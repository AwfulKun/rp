{% extends 'base.html.twig' %}

{% block title %}AppCharacter{% endblock %}

{% block body %}
    <h1>AppCharacter</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ app_character.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ app_character.name }}</td>
            </tr>
            <tr>
                <th>Surname</th>
                <td>{{ app_character.surname }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('characters_index') }}">back to list</a>

    <a href="{{ path('app_character_edit', {'id': app_character.id}) }}">edit</a>

    {{ include('app_character/_delete_form.html.twig') }}
{% endblock %}
